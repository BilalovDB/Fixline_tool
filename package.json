{
    "name": "fixline-tool",
    "displayName": "Fixline tool",
    "description": "Fixline tool",
    "version": "0.3.2",
    "publisher": "Danil",
    "icon": "images/fixline.png",
    "repository": {
        "url": "https://github.com/Finist02/Fixline_tool",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.71.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:ctrl",
        "onLanguage:ctrlpp"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.OpenPanel",
                "title": "Open panel",
                "icon": "$(preview)"
            },
            {
                "command": "extension.OpenProjectPanel",
                "title": "Open project panel",
                "icon": "$(preview)"
            },
            {
                "command": "extension.CheckScript",
                "title": "Check Script",
                "icon": "$(debug)"
            },
            {
                "command": "extension.RunScript",
                "title": "Run script",
                "icon": "$(run)"
            },
            {
                "command": "extension.OpenLogs",
                "title": "Open Log",
                "icon": "$(list-flat)"
            },
            {
                "command": "extension.Panelpreview",
                "title": "Panel preview",
                "icon": "$(open-preview)"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.OpenPanel",
                    "when": "resourceExtname == .xml",
                    "group": "navigation@6"
                },
                {
                    "command": "extension.Panelpreview",
                    "when": "resourceExtname == .xml",
                    "group": "navigation@6"
                },
                {
                    "command": "extension.CheckScript",
                    "when": "resourceExtname == .ctl",
                    "group": "navigation@6"
                },
                {
                    "command": "extension.RunScript",
                    "when": "resourceExtname == .ctl",
                    "group": "navigation@6"
                }
            ],
            "editor/title": [
                {
                    "command": "extension.OpenPanel",
                    "group": "navigation",
                    "when": "resourceExtname == .xml"
                },
                {
                    "command": "extension.Panelpreview",
                    "group": "navigation",
                    "when": "resourceExtname == .xml"
                },
                {
                    "command": "extension.CheckScript",
                    "group": "navigation",
                    "when": "resourceExtname == .ctl"
                },
                {
                    "command": "extension.RunScript",
                    "group": "navigation",
                    "when": "resourceExtname == .ctl"
                }
            ]
        },
        "configuration": [
            {
                "type": "object",
                "title": "Fixline tool",
                "properties": {
                    "FixLineTool.OpenPanel.UserName": {
                        "type": "string",
                        "default": "root",
                        "description": "User Name"
                    },
                    "FixLineTool.OpenPanel.Password": {
                        "type": "string",
                        "default": "",
                        "description": "Password"
                    }
                }
            }
        ]
    },
    "capabilities": {
        "definitionProvider": "true"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.71.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vscode/test-electron": "^2.1.5",
        "eslint": "^8.20.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "fsp-xml-parser": "^1.0.0"
    }
}
